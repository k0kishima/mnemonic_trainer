{
  "extends": "../tsconfig-base",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "$frontend/*": [
        "./src/*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src/**/*"
  ]
}
